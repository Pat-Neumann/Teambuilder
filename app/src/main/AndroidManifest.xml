<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.andrasia.teambuilder">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:name=".CheckAuthentication"
        android:allowBackup="true"
        android:icon="@mipmap/teambuilder_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/teambuilder_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LandingPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"  />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterPage"></activity>
        <activity android:name=".LoginPage"></activity>
        <activity android:name=".SearchConfigurationPage"></activity>
        <activity android:name=".SearchPage"></activity>

    </application>

</manifest>